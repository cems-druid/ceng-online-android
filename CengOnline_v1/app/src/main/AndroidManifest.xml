<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cengonline_v1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Student_General_View"></activity>
        <activity android:name=".Student_view" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Teacher_General_View" />
        <activity android:name=".Teacher_view" />
        <activity
            android:name=".AnnouncementMVVM.AnnouncementMainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".AnnouncementMVVM.AddEditAnnouncementActivity"
            android:parentActivityName=".AnnouncementMVVM.AnnouncementMainActivity" />
        <activity
            android:name=".CoursesMVVM.CourseMainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".CoursesMVVM.AddEditCourseActivity"
            android:parentActivityName=".CoursesMVVM.CourseMainActivity" />
        <activity android:name=".AssignmentMVVM.AssignmentMainActivity" />
        <activity
            android:name=".AssignmentMVVM.AddEditAssignmentActivity"
            android:parentActivityName=".AssignmentMVVM.AssignmentMainActivity" />
        <activity android:name=".PostsMVVM.PostMainActivity" />
        <activity
            android:name=".PostsMVVM.AddEditPostActivity"
            android:parentActivityName=".PostsMVVM.PostMainActivity" />
    </application>

</manifest>